<%= form_for([@country, @landmark]) do |f| %>

<div>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
</div>

<div>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
</div>

<div>
  <%= f.label :photo %><br>
  <%= f.text_area :photo %><br>
</div>

<%= f.collection_select(:country_id, Country.all, :id, :name ) %><br>

<div>
  <%= f.submit %><br>
</div>

<% end %>
